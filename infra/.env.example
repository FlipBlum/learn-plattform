# Supabase Cloud
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
SUPABASE_JWT_SECRET=your-supabase-jwt-secret

# OpenAI (for quiz/critique/news generation)
OPENAI_API_KEY=sk-your-openai-api-key

# Gmail API (for newsletter parsing)
GMAIL_CREDENTIALS_PATH=./credentials/gmail.json

# Google OAuth (for Supabase Auth)
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# API config
FRONTEND_URL=https://learn.innovique.ai

# Nuxt frontend runtime config (NUXT_PUBLIC_ prefix maps to runtimeConfig.public)
NUXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NUXT_PUBLIC_SUPABASE_KEY=your-supabase-anon-key
NUXT_PUBLIC_API_BASE_URL=
